{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "3D-RE-GEN Object Pose",
  "type": "object",
  "required": [
    "transform_matrix",
    "translation",
    "rotation_quaternion",
    "scale",
    "confidence",
    "is_floor_contact"
  ],
  "properties": {
    "transform_matrix": {
      "type": "array",
      "minItems": 4,
      "maxItems": 4,
      "items": {
        "type": "array",
        "minItems": 4,
        "maxItems": 4,
        "items": {"type": "number"}
      }
    },
    "translation": {
      "type": "array",
      "items": {"type": "number"},
      "minItems": 3,
      "maxItems": 3
    },
    "rotation_quaternion": {
      "type": "array",
      "items": {"type": "number"},
      "minItems": 4,
      "maxItems": 4
    },
    "scale": {
      "type": "array",
      "items": {"type": "number"},
      "minItems": 3,
      "maxItems": 3
    },
    "confidence": {"type": "number"},
    "is_floor_contact": {"type": "boolean"}
  },
  "additionalProperties": true
}
